   "source": [
    "#making a basic bokeh line graph\n",
    "\n",
    "from bokeh.plotting import figure, output_file, show\n",
    "\n",
    "\n",
    "x=[1,2,3,4,5]\n",
    "y=[6,7,8,9,10]\n",
    "\n",
    "output_file(\"Line.html\")\n",
    "\n",
    "f=figure(width=600, height=400)\n",
    "\n",
    "f.line(x,y)\n",
    "\n",
    "show(f)"
   ]
